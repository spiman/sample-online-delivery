    "@types/express": "^4.17.6",
    "@types/mocha": "^7.0.2",
    "@types/mongoose": "^5.7.10",
    "@types/node": "^13.11.1",
    "@types/sinon": "^9.0.0",
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.3",
    "nyc": "^15.0.1",
